{
  "name": "bcsol",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "npm run generate",
    "build": "tsc && vite build",
    "prestart": "npm run generate",
    "start": "vite dev",
    "serve": "vite preview",
    "release": "semantic-release",
    "generate": "npm run generate:protobuf && npm run generate:validation",
    "generate:protobuf": "mkdir -p src/generated/protobuf && pbjs -t static-module -w es6 -o src/generated/protobuf/index.js src/types/external.proto && pbts -o src/generated/protobuf/index.d.ts src/generated/protobuf/index.js",
    "generate:validation": "ts-json-schema-generator --path 'src/types/*.ts' --type 'ITransactionExt' -o 'src/generated/validate/external-schema.json' && node generate-validator.cjs",
    "license-report": "license-report --only=prod | node scripts/license-report.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.4",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/modifiers": "^6.0.0",
    "@dnd-kit/sortable": "^7.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@project-serum/anchor": "^0.25.0",
    "@sentry/react": "^7.10.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/wallet-adapter-backpack": "^0.1.7",
    "@solana/wallet-adapter-base": "^0.9.17",
    "@solana/wallet-adapter-brave": "^0.1.11",
    "@solana/wallet-adapter-glow": "^0.1.12",
    "@solana/wallet-adapter-ledger": "^0.9.19",
    "@solana/wallet-adapter-phantom": "^0.9.16",
    "@solana/wallet-adapter-react": "^0.15.19",
    "@solana/wallet-adapter-react-ui": "^0.9.17",
    "@solana/wallet-adapter-solflare": "^0.6.16",
    "@solana/web3.js": "^1.63.1",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "buffer": "^6.0.3",
    "chakra-ui-markdown-renderer": "^4.1.0",
    "crypto-browserify": "^3.12.0",
    "framer-motion": "^6.3.15",
    "immer": "^9.0.15",
    "js-sha256": "^0.9.0",
    "jsbi": "^3.2.5",
    "pako": "^2.0.4",
    "process": "^0.11.10",
    "protobufjs": "^7.1.2",
    "react": "^18.2.0",
    "react-aria": "^3.18.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.3.0",
    "react-icons": "^3.11.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.3.0",
    "react-stately": "^3.16.0",
    "snake-case": "^3.0.4",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/bs58": "^4.0.1",
    "@types/node": "^18.11.17",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^3.0.0",
    "ajv": "^8.11.2",
    "license-report": "^6.0.0",
    "protobufjs-cli": "^1.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-export-data": "^1.0.0",
    "ts-json-schema-generator": "^1.1.2",
    "vite": "^4.0.3",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3"
  }
}
