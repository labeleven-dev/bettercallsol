{
  "name": "bcsol",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "npm run generate",
    "build": "tsc && vite build",
    "prestart": "npm run generate",
    "start": "vite dev",
    "serve": "vite preview",
    "release": "semantic-release",
    "generate": "npm run generate:protobuf && npm run generate:validation",
    "generate:protobuf": "mkdir -p src/generated/protobuf && pbjs -t static-module -w es6 -o src/generated/protobuf/index.js src/types/external.proto && pbts -o src/generated/protobuf/index.d.ts src/generated/protobuf/index.js",
    "generate:validation": "ts-json-schema-generator --path 'src/types/*.ts' --type 'ITransactionExt' -o 'src/generated/validate/external-schema.json' && node scripts/generate-validator.cjs",
    "license-report": "license-report --only=prod | node scripts/license-report.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/react": "^2.4.4",
    "@dnd-kit/core": "^6.0.6",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@project-serum/anchor": "^0.25.0",
    "@sentry/react": "^7.38.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.3.7",
    "@solana/wallet-adapter-base": "^0.9.20",
    "@solana/wallet-adapter-brave": "^0.1.14",
    "@solana/wallet-adapter-react": "^0.15.26",
    "@solana/wallet-adapter-react-ui": "^0.9.25",
    "@solana/wallet-adapter-solflare": "^0.6.20",
    "@solana/web3.js": "^1.70.3",
    "@sqds/sdk": "^2.0.3",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.0.2",
    "buffer": "^6.0.3",
    "chakra-ui-markdown-renderer": "^4.1.0",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.7",
    "framer-motion": "^8.0.1",
    "immer": "^9.0.16",
    "js-sha256": "^0.9.0",
    "jsbi": "^3.2.5",
    "pako": "^2.1.0",
    "process": "^0.11.10",
    "protobufjs": "^7.1.2",
    "react": "^18.2.0",
    "react-aria": "^3.23.1",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.4",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.6.0",
    "react-stately": "^3.20.0",
    "snake-case": "^3.0.4",
    "stream-browserify": "^3.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/node": "^18.16.0",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^4.0.0",
    "ajv": "^8.12.0",
    "license-report": "^6.4.0",
    "protobufjs-cli": "^1.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "semantic-release": "^21.0.1",
    "semantic-release-export-data": "^1.0.1",
    "ts-json-schema-generator": "^1.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
